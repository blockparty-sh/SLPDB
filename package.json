{
  "name": "slpdb",
  "version": "0.15.3",
  "description": "Token graph data layer for Simple Ledger Protocol",
  "main": "index.js",
  "scripts": {
    "test": "tsc && test",
    "start": "tsc && node --max_old_space_size=8192 index",
    "lint": "./node_modules/.bin/eslint .",
    "migrate": "./node_modules/.bin/migrate-mongo",
    "build": "./node_modules/.bin/grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./proto --grpc_out=./proto --plugin=protoc-gen-grpc=./node_modules/.bin/grpc_tools_node_protoc_plugin -I ./proto ./proto/*.proto && ./node_modules/protoc/protoc/bin/protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=./proto -I ./proto ./proto/*.proto && tsc",
    "reprocess": "node CommandClient.js --reprocess "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simpleledger/slpdb.git"
  },
  "keywords": [
    "SLP"
  ],
  "author": "SLP Developers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simpleledger/slpdb/issues"
  },
  "homepage": "https://github.com/simpleledger/slpdb#readme",
  "dependencies": {
    "@types/argparse": "^1.0.36",
    "@types/dotenv": "^6.1.0",
    "@types/google-protobuf": "^3.7.1",
    "@types/ip": "^1.1.0",
    "@types/mongodb": "^3.1.19",
    "@types/node": "^10.12.21",
    "@types/p-limit": "2.0.0",
    "@types/p-queue": "3.1.0",
    "@types/protobufjs": "^6.0.0",
    "@types/zeromq": "^4.6.1",
    "argparse": "^1.0.10",
    "bcash": "^1.1.1",
    "bignumber.js": "^8.1.0",
    "bitbox-sdk": "8.2.1",
    "bitcoin-rpc-promise": "gist:7c752c62fdae80fc7e2125a6016cbf26",
    "bitcore-lib-cash": "^0.19.0",
    "bufio": "^1.0.4",
    "cashaddrjs-slp": "^0.2.11",
    "dotenv": "^6.0.0",
    "fountainhead-core": "0.0.4",
    "google-protobuf": "^3.9.1",
    "grpc": "1.23.1",
    "grpc-bchrpc-node": "0.6.2",
    "grpc-tools": "^1.8.0",
    "grpc_tools_node_protoc_ts": "^2.5.4",
    "iconv-lite": "^0.4.24",
    "ip": "^1.1.5",
    "level": "^5.0.1",
    "migrate-mongo": "^5.0.1",
    "mingo": "^2.2.4",
    "mongodb": "^3.1.4",
    "node-jq": "1.6.0",
    "p-limit": "2.0.0",
    "p-queue": "3.1.0",
    "protobufjs": "^6.8.8",
    "protoc": "^1.0.3",
    "slpjs": "0.21.1",
    "traverse": "^0.6.6",
    "typescript": "^3.6.2",
    "zeromq": "^5.1.0"
  },
  "devDependencies": {
    "eslint": "^5.6.1"
  }
}
